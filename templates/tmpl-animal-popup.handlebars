<form id="submitForm" onclick = "form_submit('submitForm'); return false;">

        <div class="form-group">
          <label class="control-label" for="name">Name</label>
          <input type="text" class="form-control" name="name" id="name" value="{{ name }}">
        </div>

        <div class="form-group">
          <label class="control-label" for="reader_id">Description</label>
          <input type="text" class="form-control" name="description" id="description" value="{{ description }}">
        </div>

        <div class="form-group">
          <label class="control-label" for="name">Start Timestamp</label>
          <input type="text" class="form-control" name="longitude" id="start_timestamp" value="{{ start_timestamp }}">
        </div>

        <div class="form-group">
          <label class="control-label" for="reader_id">End Timestamp</label>
          <input type="text" class="form-control" name="latitude" id="end_timestamp" value="{{ end_timestamp }}">
        </div>

        <div class="form-group">
          <div class="sumbit">
            <button class="btn btn-primary" id="submitButton" value="PUT">Submit</button>
          </div>
        </div>
</form>
