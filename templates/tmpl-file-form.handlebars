<form id="submit_file" method="POST">

        <div class="form-group" style="display:none">
          <label class="control-label" for="url">URL</label>
          <input type="text" class="form-control" name="url" id="url" value="{{ url }}">
        </div>

        <div class="form-group">
          <label class="control-label" for="reader_id">Tag ID</label>
          <input type="text" class="form-control" name="tag_id" id="tag_id" value="{{ tag_id }}">
        </div>
			<div class="form-group">
          <label class="control-label" for="file">File Upload</label>
				<input type="file" name="upload_file" id="upload_file">
		</div>
        <div class="form-group">
          <div class="sumbit">
            <button type="submit" id="submit_file_button" onclick="file_upld('submit_file'); return false;">Submit</button>
          </div>
        </div>
</form>
